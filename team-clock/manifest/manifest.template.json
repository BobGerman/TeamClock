{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.8/MicrosoftTeams.schema.json",
  "manifestVersion": "1.8",
  "version": "<APP_VERSION>",
  "id": "<TEAMS_APP_ID>",
  "packageName": "io.github.pnp.teamsdevsamples.tab-create-react-app-ts-msal",
  "developer": {
    "name": "Derek and Bob",
    "websiteUrl": "https://pnp.github.io",
    "privacyUrl": "https://<HOSTNAME>:<PORT>/#/privacy",
    "termsOfUseUrl": "https://<HOSTNAME>:<PORT>/#/termsofuse"
  },
  "icons": {
      "color": "<ICON_192>",
      "outline": "<ICON_32>"
  },
  "name": {
    "short": "Team Clock",
    "full": "Team Clock helps teams who are spread around the world"
  },
  "description": {
    "short": "Displays time for a list of people in different time zones",
    "full": "Displays time and assists with scheduling meetings with teammates who are spread across multiple time zones"
  },
  "accentColor": "#FFFFFF",
  "configurableTabs": [
    {
      "configurationUrl": "https://<HOSTNAME>:<PORT>/#/config",
      "canUpdateConfiguration": true,
      "scopes": [
        "team",
        "groupchat"
      ]
    }
  ],
  "staticTabs": [
    {
      "entityId": "<NAME>",
      "name": "<NAME>",
      "contentUrl": "https://<HOSTNAME>:<PORT>/#/tab",
      "websiteUrl": "https://<HOSTNAME>:<PORT>/#/web",
      "scopes": [
        "personal"
      ]
    },
    {
      "entityId": "Test Page",
      "name": "Test Page",
      "contentUrl": "https://<HOSTNAME>:<PORT>/#/tabTest",
      "websiteUrl": "https://<HOSTNAME>:<PORT>/#/webTest",
      "scopes": [
        "personal"
      ]
    }

  ],
  "bots": [ ],
  "showLoadingIndicator": true,
  "permissions": [
      "identity",
      "messageTeamMembers"
  ],
  "validDomains": [
      "<HOSTNAME>:<PORT>",
      "login.microsoftonline.com",
      "*.sharepoint.com",
      "spoprod-a.akamaihd.net",
      "resourceseng.blob.core.windows.net"
  ]
}